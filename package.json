{
  "name": "calvacljstestbed",
  "displayName": "CalvaCLJSTestbed",
  "description": "",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:clojure",
    "calvacljstestbed.helloWorld"
  ],
  "main": "node_modules/shadow-cljs/calva_cljs.extension.js",
  "contributes": {
    "commands": [
      {
        "command": "calvacljstestbed.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "calvacljstestbed.helloCljsLib",
        "title": "Hello cljs-lib"
      },
      {
        "command": "calvacljstestbed.helloTypeScript",
        "title": "Hello TypeScript"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile-cljs": "npx shadow-cljs compile :calva-lib",
    "release-calva-lib": "npx shadow-cljs release :calva-lib",
    "watch-ts": "npx tsc --watch --project ./tsconfig.json",
    "unit-test": "npx mocha --require ts-node/register 'src/extension-test/unit/**/*-test.ts'",
    "pretest": "npm run compile && npm run lint",
    "compile": "tsc",
    "lint": "",
    "test": "mocha",
    "clean": "rimraf ./out ./tsconfig.tsbuildinfo ./cljs-out ./lib ./src/cljs-lib/out",
    "bundle": "esbuild public/js/main.js --bundle --platform=node --packages=external --outfile=public/js/main.bundle.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.14.6",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vscode/test-electron": "^2.2.2",
    "assert": "^2.0.0",
    "esbuild": "0.17.10",
    "eslint": "^8.8.0",
    "glob": "^7.1.6",
    "mocha": "^9.2.2",
    "rimraf": "^2.7.1",
    "shadow-cljs": "^2.22.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "cljs-lib": "file:src/cljs-lib"
  }
}
